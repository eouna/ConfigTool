<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.*?>
<Pane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="520.0"
      prefWidth="800.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.eouna.configtool.ui.controllers.ConfigSettingController">
    <children>
        <VBox prefHeight="520.0" prefWidth="800.0" style="-fx-padding: 5px">
            <SplitPane prefHeight="465" dividerPositions="0.3">
                <TreeView fx:id="settingTree" style="-fx-background-color: #ffffff" prefWidth="300"/>
                <GridPane fx:id="settingArea" vgap="10"
                          style="-fx-padding: 10 5;-fx-background-color: rgba(217,238,238,0.25)"
                          prefWidth="500"/>
            </SplitPane>
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="55.0" prefWidth="790.0">
                <children>
                    <HBox alignment="CENTER" layoutX="1.0" layoutY="4.0" prefHeight="55.0" prefWidth="790.0"
                          spacing="20.0" AnchorPane.leftAnchor="0.0">
                        <children>
                            <Button mnemonicParsing="false" onAction="#onConfirm" text="确定"/>
                            <Button mnemonicParsing="false" onAction="#onCancelClick" text="取消"/>
                        </children>
                    </HBox>
                </children>
            </AnchorPane>
        </VBox>
    </children>
</Pane>
